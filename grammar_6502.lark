// start tag. * means that sentence can be repeated 0 to many times.
start: line*

?line: NEWLINE
    | label
    | numeric_val

label: IDENTIFIER ":"
numeric_val: HEX | DEC | BIN

// RAW vals
IDENTIFIER: /[A-Za-z_][A-Za-z0-9_]*/
DEC: /d+/
HEX: /\$[0-9A-Fa-f]+/
BIN: /\%[01]+/

%import common.WS_INLINE
%import common.NEWLINE
%ignore WS_INLINE


