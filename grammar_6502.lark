// start tag. * means that sentence can be repeated 0 to many times.
start: line*

?line: NEWLINE
    | label
    | ACCUMULATOR
    | numeric_val

label: IDENTIFIER ":"
ACCUMULATOR: "A"
numeric_val: HEX | DEC

// RAW vals
IDENTIFIER: /[A-Za-z_][A-Za-z0-9_]*/
DEC: /d+/
HEX: /\$[0-9A-Fa-f]+/

%import common.WS_INLINE
%import common.NEWLINE
%ignore WS_INLINE


